	pages = {35--46},
	annote = {The following values have no corresponding Zotero field:auth-address: F. Ebhodaghe, African Regional Postgraduate Programme in Insect Science, West African Sub-Regional Centre, University of Ghana Legon, Accra, Ghana. E-mail: ebhodaghefaith@gmail.compub-location: Netherlandspublisher: Elsevier B.V.accession-num: 2001149524},
