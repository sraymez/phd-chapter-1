	journal = {International Journal of Infectious Diseases},
	author = {Sow, A. and Ba, Y. and Diallo, D. and Omar, F. and Ousmane, F. and Kathryn, H. and Scott, C. W. and Mawlouth, D. and Amadou Alpha, S.},
