	pages = {60},
	annote = {The following values have no corresponding Zotero field:auth-address: S.A. Shohaimi, Veterinary Research Institute (VRI), 59, Jalan Sultan Azlan Shah, Ipoh, Perak, Malaysiatertiary-title: 5th International Conference on Tropical Medicine and Infectious Diseases 2022, ICTMID 2022. Virtual.pub-location: Netherlandspublisher: Malaysian Medical Associationaccession-num: 639680366},
